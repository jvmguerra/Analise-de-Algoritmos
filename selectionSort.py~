def selectionSort(A):
	for i in range(0,(len(A)-1)):
		minimo = i
		for j in range(i+1, len(A)):
			if A[j] < A[minimo]:
				minimo = j
		print("Trocando ", A[i], "por ", A[minimo])		
		aux = A[i]
		A[i] = A[minimo]
		
		A[minimo] = aux
	
	print(A)
A = [40, 12, 34, 1, 3, 5, 80]
selectionSort(A)
print(A)
